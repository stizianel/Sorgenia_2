		<partnerAggregatorTax>
			<mainSelect ><![CDATA[
				select acdan,                                                --0 
               apiva,                                                --1
               acdfi,                                                --2
               LENGTH(TRIM(TRANSLATE(acdfi, ' +-.0123456789',' '))), --3
               'IT'||apiva,                                          --4
               decode(length(acdfi),11,'FN',16,'CF','ER')            --5
            from v_clienti a
            where a.atipoentita = 'CLIENTE'
            and a.acdan in (select partner from partner_init)
			]]></mainSelect>
