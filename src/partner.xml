      from
      partner_init a left outer join partner_but0bk k on a.legacy = k.legacy
        left outer join partner_but021 d on a.legacy = d.legacy,
      partner_but000 b,
      partner_but020 c,
      partner_taxnum t,
      partner_butcom m
      where
      a.legacy = b.legacy and
      a.legacy = c.legacy and
      a.legacy = t.legacy and
      a.legacy = m.legacy
